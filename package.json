{
  "name": "swagjs",
  "version": "1.0.0",
  "description": "Modular Swagger Spec Builder",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "sonar": "node ./scripts/sonar",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "coveralls": "cat reports/coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "bin": {
    "swagjs": "./index.js"
  },
  "engines": {
    "node": ">=9"
  },
  "private": false,
  "keywords": [
    "swagger",
    "specs",
    "api",
    "apis",
    "documentation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/swagjs.git"
  },
  "author": "@vitorsalgado",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vitorsalgado/swagjs/issues"
  },
  "homepage": "https://github.com/vitorsalgado/swagjs#readme",
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-nodesecurity": "^1.3.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "express": "^4.16.4",
    "js-yaml": "^3.13.1",
    "nodejs-fx": "^1.1.0",
    "swagger-tools": "^0.10.4"
  },
  "jest": {
    "notify": false,
    "verbose": true,
    "clearMocks": true,
    "resetMocks": true,
    "testEnvironment": "node",
    "coverageDirectory": "reports/coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "__tests__/(fixtures|__mocks__)/",
      "/_(temp|mock|install|init|helpers|utils).js$"
    ]
  }
}
